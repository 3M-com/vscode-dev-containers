{
	"variants": ["6.0", "6.0-focal", "5.0-focal", "3.1-focal", "2.1-focal"],
	"definitionVersion": "0.201.10",
	"build": {
		"latest": "5.0-focal",
		"rootDistro": "debian",
		"architectures": ["linux/amd64"],
		"tags": [
			"dotnet:${VERSION}-${VARIANT}",
			"dotnetcore:${VERSION}-${VARIANT}"
		],
		"variantTags": {
			"6.0": ["dotnet:${VERSION}-6.0-bullseye"],
			"5.0-focal": ["dotnet:${VERSION}-5.0", "dotnetcore:${VERSION}-5.0"],
			"3.1-focal": ["dotnet:${VERSION}-3.1", "dotnetcore:${VERSION}-3.1"],
			"2.1-focal": ["dotnet:${VERSION}-2.1", "dotnetcore:${VERSION}-2.1"]
		}		
	},
	"dependencies": {
		"image": "mcr.microsoft.com/dotnet/sdk:${VARIANT}",
		"imageLink": "https://hub.docker.com/_/microsoft-dotnet-sdk",
		"apt": [{
			"cgIgnore": false,
			"name":"yarn"
		}],
		"git": {
			"Oh My Zsh!": "/home/vscode/.oh-my-zsh",
			"nvm": "/usr/local/share/nvm"
		},
		"languages": {
			".NET": {
				"cgIgnore": true,
				"versionCommand": "dotnet --version | grep -oE '[0-9]+\\.[0-9]+\\.[0-9]+' | tr -d '\\n' && echo \\\" (\\$(dotnet --info | grep -ozP 'Host.*:\\s*Version:\\s*\\K[0-9]\\.[0-9]\\.[0-9]' | tr '\\0' '\\n'))\\\"",
				"path": "/usr",
				"downloadUrl": "https://dotnet.microsoft.com/"
			}
		}
	}
}
