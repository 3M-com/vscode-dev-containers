{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"name": "Run Push Script",
			"request": "launch",
			"preLaunchTask": "vscdc cli watch",
			"skipFiles": [
				"<node_internals>/**",
				"node_modules/**"
			],
			"runtimeExecutable": "node",
			"cwd": "${workspaceRoot}",
			"program": "${workspaceFolder}/out/vscdc.js",
			"args": [
				"push",
				"debian",
				"--no-push",
				"--page", "1",
				"--page-total", "1",
				"--release", "v0.191.0",
				"--github-repo", "microsoft/vscode-dev-containers",
				"--registry", "clantz.azurecr.io",
				"--registryPath", "vscode/devcontainers",
				"--stubRegistry", "clantz.azurecr.io",
				"--stubRegistryPath", "vscode/devcontainers"
			]
		},
		{
			"type": "pwa-node",
			"name": "Run Pack Script",
			"request": "launch",
			"preLaunchTask": "vscdc cli watch",
			"skipFiles": [
				"<node_internals>/**",
				"node_modules/**"
			],
			"runtimeExecutable": "node",
			"cwd": "${workspaceRoot}",
			"program": "${workspaceFolder}/out/vscdc.js",
			"args": [
				"pack",
				"--no-clean",
				"--prep-and-package-only",
				"--release", "main",
				"--github-repo", "microsoft/vscode-dev-containers",
				"--registry", "clantz.azurecr.io",
				"--registryPath", "vscode/devcontainers",
				"--stubRegistry", "clantz.azurecr.io",
				"--stubRegistryPath", "vscode/devcontainers"
			]
		}, {
			"type": "pwa-node",
			"name": "Run CG/Info Script",
			"request": "launch",
			"preLaunchTask": "vscdc cli watch",
			"skipFiles": [
				"<node_internals>/**",
				"node_modules/**"
			],
			"runtimeExecutable": "node",
			"cwd": "${workspaceRoot}",
			"program": "${workspaceFolder}/out/vscdc.js",
			"args": [
				"info",
				"codespaces-linux",
				"--no-build",
				"--markdown",
				"--cg",
				"--overwrite",
				"--prune",
				"--release", "main",
				"--github-repo", "microsoft/vscode-dev-containers",
				"--registry", "mcr.microsoft.com",
				"--registryPath", "vscode/devcontainers"
			]
		},
		{
			"type": "pwa-node",
			"name": "Run Update Script URLs",
			"request": "launch",
			"preLaunchTask": "vscdc cli watch",
			"skipFiles": [
				"<node_internals>/**",
				"node_modules/**"
			],
			"runtimeExecutable": "node",
			"cwd": "${workspaceRoot}",
			"program": "${workspaceFolder}/out/vscdc.js",
			"args": [
				"update-script-sources",
				"v0.100.0",
				"--github-repo", "microsoft/vscode-dev-containers"
			]
		},
		{
			"type": "pwa-node",
			"name": "Run Copy Library Scripts",
			"request": "launch",
			"preLaunchTask": "vscdc cli watch",
			"skipFiles": [
				"<node_internals>/**",
				"node_modules/**"
			],
			"runtimeExecutable": "node",
			"cwd": "${workspaceRoot}",
			"program": "${workspaceFolder}/out/vscdc.js",
			"args": [
				"copy-library-scripts"
			]
		},
		{
			"type": "pwa-node",
			"name": "Run Patch",
			"request": "launch",
			"preLaunchTask": "vscdc cli watch",
			"skipFiles": [
				"<node_internals>/**",
				"node_modules/**"
			],
			"runtimeExecutable": "node",
			"cwd": "${workspaceRoot}",
			"program": "${workspaceFolder}/out/vscdc.js",
			"args": [
				"patch",
				"--patch-path", "patch/sample",
				"--registry", "clantz.azurecr.io",
				"--registryPath", "vscode/devcontainers"
			]
		},
		{
			"type": "pwa-node",
			"name": "Run Patch All",
			"request": "launch",
			"preLaunchTask": "vscdc cli watch",
			"skipFiles": [
				"<node_internals>/**",
				"node_modules/**"
			],
			"runtimeExecutable": "node",
			"cwd": "${workspaceRoot}",
			"program": "${workspaceFolder}/out/vscdc.js",
			"args": [
				"patch",
				"--all",
				"--registry", "clantz.azurecr.io",
				"--registryPath", "vscode/devcontainers"
			]
		}
	]
}